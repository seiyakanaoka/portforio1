<div class="container">
  <div class="row">
    <div class="col-md-6">
      <% @logs.each do |log| %>
        <div class="logs">
          <%= log.user.last_name %>
          <%= log.title %>
          <%= log.body %><br />
          <%= link_to log_path(log.id) do %><%= attachment_image_tag log, :log_image, size: "100x100" %><% end %>
        </div>
      <% end %>
    </div>

    <div class="col-md-6">
      サイドバー
    </div>
  </div>
</div>