<% if log.bookmarked_by?(current_user) %>
  <%= link_to log_bookmarks_path(log), method: :delete, remote: true, class: "text-dark" do %><i class="fas fa-bookmark"></i><% end %>
<% else %>
  <%= link_to log_bookmarks_path(log), method: :post, remote: true, class: "text-dark" do %><i class="far fa-bookmark"></i><% end %>
<% end %>