$("#follow_<%= @user.id %>").html("<%= j(render 'relationships/follow', user: @user) %>");
$(".follow_count_<%= @user.id %>").html('<%= link_to follow_user_path(@user), class: "text-dark" do %><%= @user.follower.count %>フォロー<% end %>');
$(".follower_count_<%= @user.id %>").html('<%= link_to follower_user_path(@user), class: "text-dark" do %><%= @user.followed.count %>フォロワー<% end %>');
