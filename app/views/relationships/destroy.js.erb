$("#follow_<%= @user.id %>").html("<%= j(render 'relationships/follow', user: @user) %>");
$(".follow_count_<%= @user.id %>").html('<%= link_to follow_user_path(@user) do %>フォロー<%= @user.follower.count %><% end %>');
$(".follower_count_<%= @user.id %>").html('<%= link_to follower_user_path(@user) do %>フォロワー<%= @user.followed.count %><% end %>');
