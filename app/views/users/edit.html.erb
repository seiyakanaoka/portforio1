<div class="container">
  <div class="row">
    <div class="col-md-12">
      <%= form_with model: @user, local: true do |f| %>
        <%= f.attachment_field :profile_image %>
        <%= f.text_field :nick_name %>
        <%= f.text_field :introduction %>
        <%= f.submit '編集する' %>
      <% end %>
    </div>
  </div>
</div>