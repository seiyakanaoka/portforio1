<% unless user.logs.empty? %>
  <div class="row mt-5 border border-light shadow-lg">
    <% user.logs.reverse.each do |user_log| %>
      <div class="col-xs-4 col-sm-4 col-md-4 col-lg-4 mt-5 mb-5 text-center user-memory">
        <%= user_log.created_at.strftime('%Y/%m/%d') %><br />
        <%= link_to log_path(user_log.id) do %><%= attachment_image_tag user_log, :log_image, size: "210x210", class: "w-100" %><% end %>
      </div>
    <% end %>
  </div>
<% else%>
  <div></div>
<% end %>